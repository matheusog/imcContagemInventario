{
	"_version": "1.7.0",
	"sap.app": {
		"id": "imc.sap.mm.contageminventario",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"ach": "MM"
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName":"imc.sap.mm.contageminventario.view.S0_App",
			"type":"XML",
			"async":true,
			"id":"appStart"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "imc.sap.mm.contageminventario.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "imc.sap.mm.contageminventario.view",
				"viewType": "XML",
				"controlId": "appStart",
				"controlAggregation": "pages",
				"clearAggregation": false,
				"transition": "slide",
				"async": true,
				"bypassed": {
					"target": "s1MainScreen"
				}
			},
			"routes": [
				{
					"name": "mainScreen",
					"pattern": "cont_invent",
					"target": [
						"s1MainScreen"
					]
				}
			],
			"targets": {
				"s1MainScreen": {
					"viewId": "s1MainScreen",
					"viewType": "XML",
					"viewName": "S1_MainScreen",
					"viewLevel": 1
				}
			}
		}
	}
}